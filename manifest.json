{
  "manifest_version": 2,
  "name": "URLex",
  "version": "1.3",
  "permissions": [
    "tabs",
    "activeTab",
    "http://127.0.0.1:5000/log",
    "https",
    "http",
    "storage"
  ],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}



